{
  "manifest_version": 2,
  "name": "xbrowser_newtab",
  "version": "1.0.2",
  "description": "漂亮清新的新标签页",
  "icons" : {
    "16"            : "img/icon16.png",
    "48"            : "img/icon48.png",
    "128"           : "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "/index.html#/"
  },
  "permissions": [
    "storage",
    "tabs",
    "settingsPrivate",
    "http://*/*",
    "https://*/*",
    { "fileSystem": ["write", "directory"] }
  ],
  "key": ""
}
